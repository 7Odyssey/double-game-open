//This is java code

int[] nums = {5,4,2,7,6,8,5,2,8,14};

for (int i = 0; i < 10; i=i+1)  {   
  nums[i] = nums[i] * nums[i];  
System.out.println(nums[i]);
}   

// This is CPP

#include <iostream>
#include <string>
using namespace std;
int main(){

int nums[10] = {5,4,2,7,6,8,5,2,8,14};

for (int i = 0; i < 10; i=i+1)  {   
  nums[i] = nums[i] * nums[i];  
cout << nums[i] << endl;
}   
}
